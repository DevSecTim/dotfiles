{
  "cleanupPeriodDays": 30,
  "autoUpdatesChannel": "stable",
  "showTurnDuration": true,
  "effortLevel": "medium",
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Bash(git diff*)",
      "Bash(git log*)",
      "Bash(git status*)",
      "Bash(ls*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm -rf:*)",
      "Read(**/.env)"
    ]
  }
}
